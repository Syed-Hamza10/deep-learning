    "def gradient(cost, func, theta = 10):\n",
    "    x, y, z = symbols('x y z', real = True)\n",
    "    func = diff(func, x)\n",
    "    x = 2\n",
    "    for _ in range(10):\n",
    "        theta = theta - (0.1)*(func)\n",
    "        print(theta)"
   ]
